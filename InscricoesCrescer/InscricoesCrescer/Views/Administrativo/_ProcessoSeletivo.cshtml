            @Html.ValidationMessageFor(model => model.SelecaoFinal)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.AulasInicio, new { @class = "col-sm-2 control-label" })
        <div class="col-sm-5">
            @Html.TextBoxFor(model => model.AulasInicio, new { @class = "form-control datefield", type = "date", required = "" })
            @Html.ValidationMessageFor(model => model.AulasInicio)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.AulasFinal, new { @class = "col-sm-2 control-label" })
