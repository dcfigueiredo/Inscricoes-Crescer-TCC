 @*@model List<InscricoesCrescer.Models.SelecaoModel>
    @using InscricoesCrescer.Models
*@

<div class="cadastro-processo-seletivo">
@using (Html.BeginForm("Salvar", "Index", FormMethod.Post, new { @class = "form-horizontal" }))
{
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.AntiForgeryToken()


    <div class="form-group">
        @Html.LabelFor(model => model.EntrevistasInicio, new { @class = "col-sm-2 control-label" })
        <div class="col-sm-5">
            @Html.TextBoxFor(model => model.EntrevistasInicio, new { @class = "form-control datefield", type = "date", required = "" })
            @Html.ValidationMessageFor(model => model.EntrevistasInicio)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.SelecaoFinal, new { @class = "col-sm-2 control-label" })
