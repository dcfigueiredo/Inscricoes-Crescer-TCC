            @Html.TextBoxFor(model => model.Ano, new { @class = "form-control datefield", type = "disable", required = "" })
            @Html.ValidationMessageFor(model => model.Ano)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.Semestre, new { @class = "col-sm-2 control-label" })
        <div class="col-sm-5">
            @Html.TextBoxFor(model => model.Semestre, new { @class = "form-control datefield", type = "disable", required = "" })
            @Html.ValidationMessageFor(model => model.Semestre)
        </div>
    </div>
      <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
